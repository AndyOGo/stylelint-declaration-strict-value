{
  "plugins": ["jest"],
  "env": {
    "jest/globals": true
  },
  "globals": {
    "testRule": "readonly",
    "testCustomAutoFixMessage": "readonly"
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true,
      "optionalDependencies": false,
      "peerDependencies": false
    }],
    "jsdoc/require-jsdoc": "off"
  }
}
