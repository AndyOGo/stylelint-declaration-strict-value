{
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": false,
    "isolatedModules": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node"
  },
}
